{
  "name": "bnet-auth-service",
  "version": "1.0.0",
  "private": true,
  "description": "Battle.net authentication service",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "npm run clean && tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint --project tsconfig.json",
    "dev": "tsc-watch --onSuccess \"npm start\" | pino-pretty",
    "test": "jest",
    "clearTestCache": "jest --clearCache",
    "start": "node start.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukemnet/bnet-auth-service.git"
  },
  "keywords": [
    "battlenet",
    "bnet",
    "authentication",
    "api",
    "backend"
  ],
  "author": "Łukasz Wójcik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lukemnet/bnet-auth-service/issues"
  },
  "homepage": "https://github.com/lukemnet/bnet-auth-service#readme",
  "dependencies": {
    "abstract-cache": "1.0.1",
    "abstract-cache-redis": "1.1.0",
    "blizzapi": "0.0.8",
    "dotenv": "8.0.0",
    "fastify": "2.5.0",
    "fastify-blipp": "2.0.0",
    "fastify-caching": "5.0.0",
    "fastify-env": "0.6.2",
    "fastify-log": "1.0.3",
    "fastify-plugin": "1.6.0",
    "fastify-redis": "3.1.1",
    "ioredis": "4.10.0"
  },
  "devDependencies": {
    "@types/ioredis": "4.0.12",
    "@types/jest": "24.0.15",
    "@types/node": "12.0.10",
    "ioredis-mock": "4.12.0",
    "jest": "24.8.0",
    "pino-pretty": "3.2.0",
    "prettier": "1.18.2",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.2",
    "tsc-watch": "2.2.1",
    "tslint": "5.18.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-immutable": "6.0.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.5.2"
  }
}
